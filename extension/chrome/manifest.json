{
  "manifest_version": 2,
  "name": "undefined",
  "description": "Description here",
  "homepage_url": "https://github-repository-url-here",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "match_about_blank": true,
      "run_at": "document_start",
      "all_frames": true,
      "js": [
        "all.js"
      ],
      "css": [
        "all.css"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "tabs",
    "storage",
    "<all_urls>",
    "webRequestBlocking",
    "webRequest",
    "unlimitedStorage"
  ],
  "browser_action": {
    "default_icon": "icon-256.png",
    "default_title": "GitHub Extension",
    "default_popup": "popup.html"
  }
}
