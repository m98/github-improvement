{
  "manifest_version": 2,
  "name": "undefined",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "match_about_blank": true,
      "run_at": "document_start",
      "all_frames":true,

      "js": [
        "all.js"
      ],
      "css": [
        "all.css"
      ]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon-256.png"
  }
}
